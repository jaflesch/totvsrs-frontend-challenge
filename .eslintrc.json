{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
      "standard",
      "prettier",
      "prettier/standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier" : ["error"]
    }
}
